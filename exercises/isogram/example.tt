#!/usr/bin/env ruby
gem 'minitest', '>= 5.0.0'
require 'minitest/autorun'
require_relative '<%= name %>'

# Test data version: <%= sha1 %>
class <%= "#{name.capitalize}Test" %> < Minitest::Test
<% test_cases.each_with_index do |test_case, index| %>

<%= test_case.comment %>
  def <%= test_case.test_name %>
    <%= test_case.skip %>
    <%= test_case.render %>
  end
<% end %>
end

<%= render_partial %>
